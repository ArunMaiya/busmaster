#Make file for building DLL

<FILENAME>.dll : <INSTALLDIR>\MINGW\Obj\wrapper_J1939.o <FILENAME>.o 
	dllwrap --def=<SRCDIR>\<FILENAME>.def \
--dllname=<SRCDIR>\<FILENAME>.dll --driver-name=gcc <INSTALLDIR>\MINGW\Obj\wrapper_J1939.o <SRCDIR>\<FILENAME>.o 


#wrapper.o : c:\test\wrapper_J1939.c c:\test\wrapper_J1939.h
#	gcc -c c:\test\wrapper_J1939.c -o C:\test\wrapper_J1939.o -mwindows

<FILENAME>.o : <SRCDIR>\<FILENAME>.c
	gcc -c <SRCDIR>\<FILENAME>.c  -o <SRCDIR>\<FILENAME>.o -mwindows


